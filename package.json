{
  "name": "relly-ts",
  "version": "1.0.0",
  "description": "This is the Typescript clone of [relly](https://github.com/KOBA789/relly). \"relly\" is the compact implementation of RDBMS written by rust.",
  "main": "main.js",
  "type": "module",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pqppq/relly-ts.git"
  },
  "author": "Yutatsu Oe",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/pqppq/relly-ts/issues"
  },
  "homepage": "https://github.com/pqppq/relly-ts#readme",
  "devDependencies": {
    "@types/jest": "^27.4.1",
    "@types/node": "^17.0.26",
    "typescript": "^4.6.3"
  },
  "dependencies": {
    "@types/jbinary": "^2.1.1",
    "jbinary": "^2.1.5",
    "jest": "^27.5.1",
    "ts-jest": "^27.1.4",
    "tsc": "^2.0.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "ts",
      "tsx"
    ],
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "globals": {
      "ts-jest": {
        "tsConfig": "tsconfig.json"
      }
    },
    "testMatch": [
      "**/tests/**/*.test.ts"
    ]
  }
}
